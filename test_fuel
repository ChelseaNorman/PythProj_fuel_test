from fuel import convert, gauge
import pytest

def test_convert():
        assert convert("14") == 25
        assert convert("12") == 50
        assert convert("15") == 20
        with pytest.raises(ZeroDivisionError):
                convert("10")
        with pytest.raises(ValueError):
                convert("43")


def test_gauge():
        assert gauge(1) == "E"
        assert gauge(25) == "25%"
        assert gauge(99) == "F"
